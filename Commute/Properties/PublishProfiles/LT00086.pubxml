    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="context" Order="1" Enabled="True">
          <Destination Path="Data Source=LT00086;Initial Catalog=Commute;Persist Security Info=True;User ID=sa;Password=sqlserver2012" />
