@model IEnumerable<Commute.Models.Route>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table>
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Id)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsSeek)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartLatitude)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.StartLongitude)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndLatitude)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EndLongitude)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.ActionLink(item.Id.ToString(), "CreateUpdate", "Route", new { id = @item.Id }, null)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsSeek)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartLatitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StartLongitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndLatitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.EndLongitude)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
