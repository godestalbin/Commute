<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="Commute" content="Share your car to commute between home and work with others or find a car in your area to commute to work. Going to work can be fun: Save fuel, save money and make new friends to share a happy time when commuting from home to work and work to home. This is the green way to go to work" />
    <link rel="icon" type="image/png" href="/Blue car 32px">
    <link rel="shortcut icon" type="image/png" href="/Blue car 32px">

    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    @RenderBody()

    @Scripts.Render("~/bundles/jquery")
    @RenderSection("scripts", required: false)
</body>
</html>
